<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.VIBRATE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_triary"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_triary"
        android:supportsRtl="true"
        android:theme="@style/MyTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".start.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".mainscreen.MainScreenActivityImpl"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity android:name=".mainscreen.cardio.RunningDetailActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity android:name=".mainscreen.power.detail.PowerTrainingDetailActivity"
            android:windowSoftInputMode="adjustPan">
        </activity>

        <activity android:name=".network.NetworkTest">

        </activity>
    </application>

</manifest>